def list(listlink,mac,token,portal, blacklist, miip2):
    
    kategori=""
    canal=""
    veri=""
    veri_check=0
    lista=""
    test=1
    while True:
        try:
            res = requests.get(listlink,  headers=hea2(mac,token,portal), timeout=5, allow_redirects=False,stream=True)
                  
            if res.status_code == 200:
                #print("test")
                veri=str(res.text)
                veri=veri.replace("{", "")
                exec_process=1
                veri_check=1
                #print("""      âœ… Servidor disponible""")
                break                                    
            else:            
                exec_process=0
                veri_check=0
                break               
                #print("""      âš ï¸ Servidor no disponible""")                		                            
        except:
            exec_process=0
            veri_check=0
            break           
        
    if veri_check == 1:
        
        if veri.count('title":"')>1:
            for i in veri.split('title":"'):
                
                try:
                    canal=""
                    canal= str((i.split('"')[0]).encode('utf-8').decode("unicode-escape")).replace('\/','/')
                    exec_process=1
                except:
                    exec_process=0
                    #print("""      âš ï¸ Canales no disponibles""")                		                            
                    
                    

                canal=canal.replace("{", "")				
                kategori=kategori+canal+"\n"
	
    list=kategori
    
    if exec_process==1:   
        for c in canales:	               
            if c in list:                                             
                exec_process=1               
                lista="ok"               
                break  
            
    if lista == "":
        exec_process=0 
    else:
        
        exec_process=1    
          	        	 	
    return list, exec_process
		
    
